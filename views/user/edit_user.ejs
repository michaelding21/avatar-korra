<!DOCTYPE html>
<%-include('../partials/header');-%>
<%-include('../partials/navbar')-%>
<h1 id="user">The User</h1>
<br>
<h2 id="user">Update <%= user.name %></h2>

<form action="/user/<%= user.id%>?_method=PUT" method="POST" id="user">
<ol>
<% Object.keys(user).forEach(function(key){ %>
	<li><%=key%>: <input type="text" name="<%=key%>" value="<%= user[key] %>" id="user">
	</li>
<% }); %>
</ol>
	<button id="user">Update User</button>
</form>

<%-include('../partials/footer')-%>
